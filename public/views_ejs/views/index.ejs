<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="/socket.io/socket.io.js"></script>
    <script src="/main.js" defer type="module"></script>รง
    <script src="https://cdn.jsdelivr.net/npm/normalizr@3.6.1/dist/normalizr.browser.min.js
    " defer type="module"></script>
    <link rel="stylesheet" href="/styles.css">
    <title>Document</title>
</head>
<body>
    <h1>NUESTROS PRODUCTOS</h1>
    <% if (productos !== [] ) { %>
        <table>
            <thead>
                <th>TITLE</th>
                <th>PRICE</th>
                <th>IMAGE</th>
            </thead>
            <tbody>
                <% productos.forEach((prod)=> { %>
                <tr>
                    <td><%= prod.title %></td>
                    <td>$ <%= prod.price %></td>
                    <td><img style="width: 50px; height:50px" src=<%= prod.thumbnail %>></td>
                </tr>
                <% }) %>
            </tbody>
    </table>
    <%}else{%>
        <p>NO DATA!</p>
    <% } %>
    <a href="http://localhost:8080/api/">VOLVER</a>



</body>
</html>